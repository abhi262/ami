package emp;
class A{
	//narrowing
	byte a;
	int b=1234;
	float e=1.22F;
	int f;
	long r;
	//widening
	int c;
	byte d=12;
	void cast() {
	 a=(byte)b;
	 c=d;
	 f=(int)e;
	 r=(long)e;
	 System.out.println("narrowing :"+a);
	 System.out.println("shadowing :"+c);
	 System.out.println("narrowing for float:"+f);
	 System.out.println("widening of float in long :"+r);
	}
}
public class Typecast {
	public static void main(String[] args) {
		A obj=new A();
		obj.cast();
	}
}
